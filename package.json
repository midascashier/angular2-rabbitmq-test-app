{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run live\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "live": "live-server --quiet --entry-file=index.html --port=55600",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.11",
    "systemjs": "0.19.24",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.4",
    "bootstrap": "~3.3.6",
    "jquery": "~2.2.2",
    "stompjs": "~2.3.3"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.9"
  }
}
